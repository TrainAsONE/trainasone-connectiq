<layouts>
  <layout id="DownloadLayout">
    <drawable id="banner"/>
    <label id="message" x="center" y="75" font="Gfx.FONT_SMALL"
      justification="Gfx.TEXT_JUSTIFY_CENTER" text="@Strings.downloadingWorkoutString" />
  </layout>
  <layout id="ErrorLayout">
    <drawable id="banner"/>
    <label id="message" x="center" y="75" justification="Gfx.TEXT_JUSTIFY_CENTER" />
  </layout>
  <layout id="GrantLayout">
    <drawable id="banner"/>
    <label id="message" x="center" y="75" font="Gfx.FONT_SMALL"
      justification="Gfx.TEXT_JUSTIFY_CENTER" />
  </layout>
  <layout id="WorkoutLayout">
    <drawable id="banner"/>
    <label id="message" x="center" y="75" font="Gfx.FONT_SMALL"
      justification="Gfx.TEXT_JUSTIFY_CENTER" />
  </layout>
</layouts>
